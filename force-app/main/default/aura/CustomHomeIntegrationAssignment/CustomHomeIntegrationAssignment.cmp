<aura:component controller="CommunityAssignmentController" implements="force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,forceCommunity:availableForAllPageTypes,force:lightningQuickAction" access="global">
    <aura:attribute name="contactName" type="String" access="PRIVATE"/>   <!-- list for getting all object from apex -->
    <aura:attribute name="userName" type="String" access="PRIVATE"/>   <!-- list for getting all object from apex -->
    <aura:attribute name="userId" type="String" access="PRIVATE"/>
    <aura:attribute name="profilePic" type="String" access="PRIVATE"/>   <!-- list for getting all object from apex -->
    <aura:handler name="init" value="{!this}" action="{!c.init}" />
    <div style="display: flex;
    flex-direction: column;
    height: 100vh;
    background-color: aqua;">

        <div class="image" style="display: flex;
        flex-direction: row;
        justify-content: center;
        margin-top: 1rem;">
          <img style="max-width: 13rem;
          height: 13rem;
          border-radius: 50%;"  src="data:image/png;base64,"/>
        </div>
            <div style="font-size: 2rem;font-size: 2rem;
            display: flex;
            flex-direction: column;
            align-items: center;
            margin-top: 2rem;">
                <div>
                <span>Contact Name- </span> <span>Welcome {!v.contactName}</span>
            </div>
            <div>
                <span>User Name-  </span> <span>{!v.userName}</span>
            </div>

                <!-- <h1>Contact Name- </h1>
                <p>Welcome {!v.contactName}</p>
                <h1>User Name- </h1>
                <p> {!v.userName}</p> -->
                <!-- <h1>User Id- </h1>
                <p> {!v.userId}</p> -->
            </div>
    </div>
</aura:component>